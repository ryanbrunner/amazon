

<%= form_for book, multipart: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :quantity %>
  <%= f.text_field :quantity %>

  <%= f.label :cover %>
  <%= f.text_field :cover, type: 'filepicker', 'data-fp-apikey' => "ArHkOyrXSpyYRo35YstVHz" %>
  
  <%= f.submit %>
<% end %>